{
  "name": "lower-albina",
  "description": "exploratory mapping project",
  "version": "0.0.0",
  "author": "Nikolas Wise <hello@nikolas.io> (http://nikolas.io/)",
  "devDependencies": {
    "6to5ify": "^4.1.0",
    "browserify": "^8.1.3",
    "chroma-js": "^0.6.3",
    "fsmonitor": "^0.2.4",
    "imageoptim-cli": "^1.11.4",
    "live-server": "^0.6.3",
    "node-sass": "^2.0.0-beta",
    "parallelshell": "^1.0.4"
  },
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolaswise/lower-albina.git"
  },
  "scripts": {
    "compile": "browserify -e src/js/index.js -d -t [ 6to5ify --sourceMapRelative .] -o dist/bundle.js",
    "dev": "parallelshell 'live-server' 'npm run watch:js' 'npm run watch:sass'",
    "img": "imageoptim -d src/img/",
    "predev": "npm run prepublish",
    "prepublish": "npm run compile && npm run sass",
    "sass": "node-sass -r src/sass/style.scss -o dist/",
    "watch:js": "fsmonitor -d src/js/ npm run compile",
    "watch:sass": "node-sass -w -r src/sass/style.scss -o dist/"
  }
}
